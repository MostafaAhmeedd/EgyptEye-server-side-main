<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>EgyptEye-Profile</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!----======== CSS ======== -->
    <link href="styles.css" rel="stylesheet">
    <link href="profile.css" rel="stylesheet">
    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css">
</head>

<body>

     <nav class="nav">
        
         <ul class="nav-links">
            <li><a href="profile.html">
                <i class="uil uil-user"></i>
                <span class="link-name">Profile</span>
                <a href="main.html">
                    <i class="uil uil-home"></i>
                    <span class="link-name">Home</span>
                </a>
            </a></li>
            <li>
                <img src="logoo.png"    width="100" height="100">
            </li>
            <li><a href="login.html">
                <i class="uil uil-signout"></i>
                <span class="link-name">Logout</span>
            </a></li>
         </ul>
     </nav>

     


     <section class="dashboard">
        <div class="formbg-inner padding-horizontal--48">
            
            <form class="form">
                <div class="title">
                    <span class="padding-bottom--15">Profile Settings</span>
                </div>
                <div class="field">
                    <div class="firstName">
                        <label>First Name</label>
                        <input type="text" name="First-Name" maxlength="10" minlength="4" >
                    </div>
                    <div class="lastName">
                        <label>Last Name</label>
                        <input type="text" name="Last-Name" maxlength="10" minlength="4" >
                    </div>    
                </div>

                <div class="field">
                    <div class="firstName">
                        <label>Email</label>
                        <input type="email" name="email" maxlength="30" minlength="4" >
                    </div>
                    <div class="lastName">
                        <label>Phone Number</label>
                        <input type="tel" name="Phone-Number" maxlength="12" minlength="4" >
                    </div>
                </div>

                <div class="field">
                    <div class="firstName">
                        <label>birthday</label>
                        <input type="date" name="birthday" maxlength="12" minlength="4" >
                    </div>
                    <div class="lastName">
                        <label>Address</label>
                        <input type="text" name="Address"  minlength="6" >
                    </div>
                </div>

                <div class="field">
                    <input type="submit" name="submit" value="Add">
                </div>
            </form>
        </div>
     </section>

     

     
       
</body>

</html>