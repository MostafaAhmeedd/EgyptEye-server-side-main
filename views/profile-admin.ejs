  <section class="dashboard">
    <div class="formbg-inner padding-horizontal--48">
      <form class="form">
        <div class="title">
          <span class="padding-bottom--15">Profile details</span>
        </div>
        <div class="field">
          <div class="firstName">
            <label>First Name</label>
            <input type="text" name="firstName" maxlength="10" minlength="4" readonly value="<%= user.firstName %>">
          </div>
          <div class="lastName">
            <label>Last Name</label>
            <input type="text" name="lastName:" maxlength="10" minlength="4" readonly value="<%= user.lastName %>">
          </div>
        </div>

        <div class="field">
          <div class="firstName">
            <label>Email</label>
            <input type="text" name="email" maxlength="10" minlength="4" readonly value="<%= user.email %>">
          </div>
          <div class="lastName">
            <label>Phone Number</label>
            <input type="text" name="phoneNumber" maxlength="10" minlength="4" readonly value="<%= user.phoneNumber %>">
          </div>
        </div>

          </section>
